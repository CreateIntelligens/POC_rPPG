# VitalLens API 設定
# 請將此檔案重新命名為 .env 並填入您的實際 API Key

# VitalLens API Key - 從 https://www.rouast.com/api/ 獲取
VITALLENS_API_KEY=your_api_key_here

# Gradio 介面設定
DEFAULT_METHOD=POS (免費)
GRADIO_SERVER_PORT=7860
GRADIO_SERVER_NAME=0.0.0.0
GRADIO_SHARE=false

# 介面自訂設定
APP_TITLE=VitalLens 生命體徵檢測器
APP_THEME=soft

# 檔案上傳限制
MAX_FILE_SIZE_MB=100

# 進階設定
ENABLE_QUEUE=true
MAX_QUEUE_SIZE=10
